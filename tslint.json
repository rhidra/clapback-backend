{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark":  [true, "single"],
    "trailing-comma": [false],
    "no-console": [false, "log"],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "ban-types": false,
    "member-access": false
  },
  "rulesDirectory": []
}
